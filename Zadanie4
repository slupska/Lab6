#include <iostream>
#include <string>

using namespace std;

int main()
{
    int n;
    string s;
    string k;
    cout << "Podaj zdanie: ";
    cin >> s;
    cout << "Podaj liczbe powtorzen: ";
    cin >> n;

    for (int i = 0; i < n; i++)
    {
        k += s;
    }

    cout << "Wynik: " << k << endl;

    return 0;
}
